public enum AttachStatus {
    ACTIVE("Active"),
    INACTIVE("Inactive"),
    DELETED("Deleted")

    final String value
    AttachStatus(String value) {
        this.value = value
    }
    String toString() { value }
    String getKey() { name() }
}