class AccessEvent {
    String fmenuText
    String description
    Long requestMapId
    boolean hasAccess
    Long id
    List<AccessSubEvent> subEvent

    AccessEvent() {
    }

    AccessEvent(String fmenuText, String description, Long requestMapId) {
        this.fmenuText = fmenuText
        this.description = description
        this.requestMapId = requestMapId
    }
}
