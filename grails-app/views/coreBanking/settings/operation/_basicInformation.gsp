<div class="row">
    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
        <div class="user-profile row" id="clientBasicInfo">
            <div class="col-sm-8 col-xs-8">
                <div class="profile-user-info profile-user-info-striped">

                    <div class="profile-info-row">
                        <div class="profile-info-name" style="width: 150px">Account Category</div>
                        <div class="profile-info-value" style="margin-left: 150px;">
                            <span id="accountCategoryId" style="display: inline;">${accountCategory ? accountCategory : 'No Data'}</span>
                        </div>
                    </div>

                    <div class="profile-info-row">
                        <div class="profile-info-name" style="width: 150px">Account Type</div>
                        <div class="profile-info-value" style="margin-left: 150px;">
                            <span id="age" style="display: inline;">${accountType ? accountType : 'No Data'}</span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-sm-4 col-xs-4"></div>
        </div>
    </div>
</div>
